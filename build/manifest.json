{
  "name": "Medium to Markdown",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": ["tabs", "storage", "activeTab"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {},
  "content_scripts": [
    {
      "matches": ["https://medium.com/p/*"],
      "js": ["messenger.js"]
    }
  ]
}
